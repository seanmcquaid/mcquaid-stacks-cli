{
  "name": "@mcquaid-stacks/cli",
  "version": "0.0.2",
  "private": false,
  "description": "CLI for generating new TypeScript projects quickly",
  "license": "MIT",
  "author": "Sean McQuaid",
  "main": "dist/index.js",
  "bin": {
    "create-mcquaid-stack": "./dist/index.js"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsup && shx cp -r ./src/templates dist",
    "changeset": "changeset",
    "release": "pnpm build && changeset publish --access public",
    "start": "ts-node src/index.ts"
  },
  "dependencies": {
    "@inquirer/prompts": "^3.1.1",
    "ejs": "^3.1.9",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/ejs": "^3.1.2",
    "@types/node": "^20.6.2",
    "@types/shelljs": "^0.8.12",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.16.0"
  }
}
